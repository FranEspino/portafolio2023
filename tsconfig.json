  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/assets/Itemsnavbar.js", "pages/index.jsx", "src/components/Toggle.jsx", "src/components/Navbar.jsx", "src/components/Banner.jsx", "src/components/Container.tsx", "pages/blog.jsx", "src/components/BottomTab.jsx"],
  "exclude": ["node_modules"]
}
